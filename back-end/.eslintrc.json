{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "eslint:recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console" : "off",
        "import/first" : "off",
        "class-methods-use-this" : "off"
    }
}
